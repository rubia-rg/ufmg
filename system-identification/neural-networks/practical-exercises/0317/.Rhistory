fxc2<-fnormal1var(xrange,m2,s2)
par(new=T)
plot(xrange, fxc1, col='red', type='l', xlim=c(0,6), ylim=c(0,1), xlab='',ylab='')
par(new=T)
plot(xrange, fxc2, col='blue', type='l', xlim=c(0,6), ylim=c(0,1), xlab='x', ylab='p(x|c1), p(x|c2)')
