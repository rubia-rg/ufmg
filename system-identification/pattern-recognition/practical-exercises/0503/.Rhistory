mykde <- function(xi, X)
{
N <- dim(X)[1]
n <- dim(X)[2]
h <- 1.06*sd(X)*(N^(-1/5))
px <- (1/(N*(2*pi*h)^n))*
sum(exp(-((xi-X)^2)/((2*h)^2)))
return(px)
}
fnormal1var <- function(x,m,r)
